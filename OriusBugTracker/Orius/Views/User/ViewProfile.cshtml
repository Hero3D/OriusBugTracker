@model Orius.Models.UserModel

@{
    ViewBag.Title = "View Profile";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<section class="panel col-md-8 col-md-offset-2">
    <header class="panel-heading">
        <h2 class="panel-title">@Model.Username</h2>
        <p class="panel-subtitle">Administrator</p>
    </header>
    <div class="panel-body">
        <div class="thumb-info">
            <div class="col-lg-2">
                <figure class="profile-picture">
                    <img src="~/assets/images/pfp.jpg" alt="Joseph Doe" class="img-circle" data-lock-picture="/assets/images/!logged-user.jpg" />
                </figure>
            </div>
            <div class="col-lg-3 col-lg-offset-3">
                <p>Id: @Model.Id</p>
                <p>Email: @Model.EmailAddress</p>
            </div>

        </div>

    </div>
</section>


